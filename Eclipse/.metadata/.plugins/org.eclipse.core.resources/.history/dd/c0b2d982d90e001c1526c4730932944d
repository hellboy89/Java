
public class ClasseBanco {

	public int numConta;
	protected String tipo;
	private String dono;
	private float saldo;
	private String status;
			
	public ClasseBanco(int numConta, String tipo, String dono, float saldo, String status) {
		super();
		this.numConta = numConta;
		this.tipo = tipo;
		this.dono = dono;
		this.saldo = saldo;
		this.status = status;
	}
	
	public int getNumConta() {
		return numConta;
	}

	public void setNumConta(int numConta) {
		this.numConta = numConta;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public String getDono() {
		return dono;
	}

	public void setDono(String dono) {
		this.dono = dono;
	}

	public float getSaldo() {
		return saldo;
	}

	public void setSaldo(float saldo) {
		this.saldo = saldo;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}
	
	public void abrirConta() {
		
	}
	
	public void fecharConta() {
		
	}
	
	public void depositar() {
		
	}
	
	public void sacar() {

	}
	
	public void pagarMensalidade() {
		
	}

	
	
	@Override
	public String toString() {
		return "ClasseBanco [getNumConta()=" + getNumConta() + ", getTipo()=" + getTipo() + ", getDono()=" + getDono()
				+ ", getSaldo()=" + getSaldo() + ", getStatus()=" + getStatus() + "]";
	}

	public static void main(String[] args) {

		// TODO Auto-generated method stub

	}

}
