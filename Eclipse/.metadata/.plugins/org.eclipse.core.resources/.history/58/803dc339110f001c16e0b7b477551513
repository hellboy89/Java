
public class ClasseBanco {

	public int numConta;
	protected String tipo;
	private String dono;
	private float saldo;
	private boolean status;
	
	public ClasseBanco(int numConta, String tipo, String dono, float saldo, boolean status) {
		this.numConta = numConta;
		this.tipo = tipo;
		this.dono = dono;
		this.saldo = saldo = 0;
		this.status = status = false;
	}
	
	public int getNumConta() {
		return numConta;
	}

	public void setNumConta(int numConta) {
		this.numConta = numConta;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public String getDono() {
		return dono;
	}

	public void setDono(String dono) {
		this.dono = dono;
	}

	public float getSaldo() {
		return saldo;
	}

	public void setSaldo(float saldo) {
		this.saldo = saldo;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(boolean status) {
		this.status = status;
	}
	
	public void abrirConta() {
		
	}
	
	public void fecharConta() {
		
	}
	
	public void depositar() {
		
	}
	
	public void sacar() {

	}
	
	public void pagarMensalidade() {
		
	}

	
	
	public void status() {
		System.out.println("O número da conta é " + this.getNumConta());
		System.out.println("O tipo de conta é " + this.getTipo());
		System.out.println("O nome do dono é " + this.getDono());
		System.out.println("O saldo é no valor de  " + this.getSaldo());
		System.out.println("O status da conta é " + this.getStatus());
	}

	public static void main(String[] args) {

		// TODO Auto-generated method stub

	}

}
